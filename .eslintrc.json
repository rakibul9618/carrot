{
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "extends": ["airbnb", "plugin:react/recommended","plugin:import/errors", "plugin:import/warnings", "plugin:css-modules/recommended"],
    "plugins": ["react", "sort-keys-fix", "jsx-a11y", "@babel", "css-modules"],
    "rules": {
      "react/button-has-type": [0],
      "import/no-named-as-default": 0,
      "indent": ["error", 2, {"ignoredNodes": ["JSXElement", "JSXElement > *", "JSXAttribute", "JSXIdentifier", "JSXNamespacedName", "JSXMemberExpression", "JSXSpreadAttribute", "JSXExpressionContainer", "JSXOpeningElement", "JSXClosingElement", "JSXText", "JSXEmptyExpression", "JSXSpreadChild"]}],
      "react/jsx-indent": ["error", 2],
      "react/jsx-indent-props": ["error", 2],
      "react/prefer-stateless-function": [0],
      "react/state-in-constructor": [0],
      "no-tabs": [0],
      "no-mixed-spaces-and-tabs": [0],
      "react/prop-types": [0],
      "react/no-did-update-set-state": [0],
      "react/jsx-filename-extension": [0],
      "react/require-default-props": [0],
      "no-nested-ternary": [0],
      "sort-keys-fix/sort-keys-fix": "warn",
      "linebreak-style": 0,
      "eslint linebreak-style": [0, "error", "windows"],
      "semi": ["error", "always"],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline"
        }
      ],
      "object-curly-spacing": ["error", "always"],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1
        }
      ],
      "eol-last": ["error", "always"],
      "no-param-reassign": ["warn"],
      "arrow-body-style": ["error", "as-needed"],
      "arrow-parens": [
        "warn",
        "as-needed",
        {
          "requireForBlockBody": false
        }
      ],
      "arrow-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "constructor-super": "error",
      "max-len": ["off"],
      "max-lines-per-function": ["off"],
      "no-const-assign": "error",
      "no-dupe-class-members": "error",
      "no-duplicate-imports": "error",
      "no-new-symbol": "error",
      "no-this-before-super": "error",
      "no-useless-computed-key": "error",
      "no-useless-constructor": "warn",
      "no-useless-rename": [
        "error",
        {
          "ignoreDestructuring": false,
          "ignoreImport": false,
          "ignoreExport": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["__REDUX_DEVTOOLS_EXTENSION__", "_id", "_doc"]
        }
      ],
      "no-var": "warn",
      "prefer-arrow-callback": [
        "warn",
        {
          "allowUnboundThis": true
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "all",
          "ignoreReadBeforeAssign": true
        }
      ],
      "prefer-template": "error",
      "require-yield": "error",
      "rest-spread-spacing": ["error", "never"],
      "template-curly-spacing": "error",
      "yield-star-spacing": ["error", "after"],
      "prefer-spread": "error",
      "generator-star-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions":0,
      "jsx-a11y/no-noninteractive-element-interactions":0,
      "jsx-a11y/control-has-associated-label":0,
      "jsx-a11y/label-has-associated-control":0,
      "jsx-a11y/interactive-supports-focus":0,
      "symbol-description": "error",
      "sort-keys": ["error", "asc"],
      "react/no-danger": 0,
      "react/display-name":0,
      "camelcase": 0,
      "react/jsx-pascal-case": "error",
      "react/jsx-props-no-spreading":0,
      "react/no-array-index-key":0,
      "react/jsx-closing-bracket-location": "error",
      "react/void-dom-elements-no-children": "error",
      "react/jsx-tag-spacing": "error",
      "react/jsx-wrap-multilines": "error",
      "react/jsx-no-comment-textnodes": "error",
      "jsx-a11y/anchor-is-valid": 0,
      "import/prefer-default-export": "off"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "src/"]
        }
      }
    }
  }
